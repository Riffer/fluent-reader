{
  "name": "fluent-reader",
  "version": "1.4.0",
  "description": "Modern desktop RSS reader (fork with updated components and extensions)",
  "main": "./dist/electron.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.map",
    "!dist/**/*.LICENSE.txt",
    "!dist/fontlist"
  ],
  "scripts": {
    "build_legacy": "SET NODE_OPTIONS=--openssl-legacy-provider && webpack --config ./webpack.config.js",
    "build": "webpack --config ./webpack.config.js --mode production",
    "build-dev": "webpack --config ./webpack.config.js --mode development",
    "electron": "SET NODE_OPTIONS= && electron ./dist/electron.js",
    "start": "cls && npm run build-dev && npm run electron",
    "format": "prettier --write .",
    "package-win": "SET NODE_OPTIONS=--openssl-legacy-provider && electron-builder -w appx:x64 && electron-builder -w appx:ia32 && electron-builder -w appx:arm64",
    "package-win-ci-legacy": "SET NODE_OPTIONS=--openssl-legacy-provider && electron-builder -w --x64 -p never && electron-builder -w --ia32 -p never",
    "package-win-ci-x64": "npm run build && electron-builder -w --x64 -p never",
    "package-win-portable": "npm run build && electron-builder --win --x64 --dir",
    "package-mac": "npm run build && electron-builder --mac --x64",
    "package-mas": "bash build/resignAndPackage.sh",
    "package-linux": "npm run build && electron-builder --linux --x64 -p never"
  },
  "keywords": [],
  "author": "Haoyuan Liu (original), Riffer (fork)",
  "license": "BSD-3-Clause",
  "repository": "github:Riffer/fluent-reader",
  "maintainers": [
    "Kai Posadowsky"
  ],
  "devDependencies": {
    "@extractus/article-extractor": "^8.0.20",
    "@fluentui/react": "^7.204.0",
    "@seald-io/nedb": "^4.0.2",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^24.10.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/simple-peer": "^9.11.9",
    "copy-webpack-plugin": "^13.0.1",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "electron-react-devtools": "^0.5.3",
    "electron-store": "^8.1.0",
    "electron-window-state": "^5.0.3",
    "font-list": "^2.0.1",
    "html-webpack-plugin": "^5.6.5",
    "js-md5": "^0.8.3",
    "lovefield": "^2.1.12",
    "prettier": "^3.6.2",
    "qrcode.react": "^1.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl-universal": "^2.2.5",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools": "^3.5.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rss-parser": "^3.13.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "better-sqlite3": "^12.4.6",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "simple-peer": "^9.11.1"
  }
}
